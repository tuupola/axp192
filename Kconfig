    config AXP192_DCDC13_LDO23_CONTROL_BIT6_SELECTED
        bool "Enable EXTEN"
    config AXP192_DCDC13_LDO23_CONTROL_BIT4_SELECTED
        bool "Enable DC-DC2"
    config AXP192_DCDC13_LDO23_CONTROL_BIT3_SELECTED
        bool "Enable LDO3"
