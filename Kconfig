        bool "Enable LDOIO0"
    config AXP192_DCDC13_LDO23_CONTROL_BIT1_SELECTED
        bool "Enable DC-DC3"
    config AXP192_DCDC13_LDO23_CONTROL_BIT0_SELECTED
        bool "Enable DC-DC0"
endmenu

