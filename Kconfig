            default 0x90 if AXP192_GPIO0_LDOIO0_VOLTAGE_BIT74_2V7
            default 0xa0 if AXP192_GPIO0_LDOIO0_VOLTAGE_BIT74_2V8
            default 0xb0 if AXP192_GPIO0_LDOIO0_VOLTAGE_BIT74_2V9
